{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "CI=false npm run build",
        "outputDirectory": "build",
        "devCommand": "npm run dev"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/build/static/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/build/favicon.ico"
    },
    {
      "src": "^/manifest.json",
      "dest": "/build/manifest.json"
    },
    {
      "src": "^/discord-competition-4",
      "dest": "/build/index.html"
    },
    {
      "src": "^/login",
      "dest": "/build/index.html"
    },
    {
      "src": "^/.*",
      "dest": "/build/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 